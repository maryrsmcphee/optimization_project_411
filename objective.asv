function dist_coord = dist_2_coords(u, coord_set)
    d_x = 0; 
    d_y = 0; 
    for i = 1:size(coord_set, 1)
        d_x = d_x + abs(u(1)-coord_set(1, i));
        d_y = d_y + abs(u(2)-coord_set(2, i));
    end
    dist_coord = sqrt(d_x^2+d_y^2); 
end